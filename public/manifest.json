{
  "name": "Fundbox",
  "version": "0.0.2",
  "minimum_chrome_version": "37",
  "manifest_version": 2,
  "description": "A fund management Chrome extension.",
  "icons": {
    "16": "icons/icon_16.png",
    "64": "icons/icon_64.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage",
    "*://*.1234567.com.cn/*",
    "*://*.163.com/*"
  ],
  "browser_action": {
    "default_title": "fundbox",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://fundgz.1234567.com.cn https://8.163.com; object-src 'self'"
}