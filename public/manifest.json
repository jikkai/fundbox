{
  "name": "Fundbox",
  "version": "0.0.1",
  "minimum_chrome_version": "37",
  "manifest_version": 2,
  "description": "A fund management Chrome extension.",
  "icons": {
    "16": "icons/icon_16.png",
    "64": "icons/icon_64.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage",
    "*://*.github.com/*"
  ],
  "browser_action": {
    "default_title": "fundbox",
    "default_popup": "popup.html"
  }
}